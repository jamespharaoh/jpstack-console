<plugin
	name="console-helper"
	package="wbs.console.helper">

	<layer name="console">
		<component class="ConsoleObjectManagerImplementation"/>
		<component class="ConsoleHelperBuilder"/>
		<component class="ConsoleHelperProviderBuilder"/>
		<component class="ConsoleHelperProviderManager"/>
		<component class="ConsoleHelperProviderMetaBuilder"/>
		<component class="ConsoleHelperProviderRegistry"/>
