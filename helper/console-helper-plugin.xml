<plugin
	name="console-helper"
	package="wbs.console.helper">

	<layer name="object-generate">

		<component class="generate.ConsoleHelperGenerator"/>

	</layer>

	<layer name="console">

		<bootstrap-component class="spec.ConsoleHelperProviderSpec"/>
		<bootstrap-component class="spec.PrivKeySpec"/>
		<bootstrap-component class="spec.RunPostProcessorSpec"/>

		<component class="builder.PrivKeyBuilder"/>

		<component class="core.ConsoleHelperImplementation"/>

		<component class="manager.ConsoleObjectManagerImplementation"/>

		<component class="provider.ConsoleHelperProviderBuilder"/>
		<component class="provider.ConsoleHelperProviderManager"/>
		<component class="provider.ConsoleHelperProviderMetaBuilder"/>
		<component class="provider.GenericConsoleHelperProvider"/>

		<component class="spec.ConsoleHelperProviderSpecManager"/>

	</layer>

</plugin>
